{
  "name": "koa-redis",
  "description": "My fork of koa-redis middleware/cache with Sentinel and Cluster support (now using official node-redis library instead of ioredis).",
  "version": "5.0.0",
  "author": "mattduffy <mattduffy@gmail.com>",
  "bugs": {
    "url": "https://github.com/mattduffy/koa-redis/issues"
  },
  "contributors": [
    "Matthew Duffy <mattduffy@gmail.com>",
    "dead_horse <dead_horse@qq.com>",
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "co-wrap-all": "^1.0.0",
    "debug": "^4.1.1",
    "eslint-config-airbnb-base": "15.0.0",
    "ioredis": "^4.14.1",
    "redis": "^5.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "autod": "^3.1.0",
    "co-mocha": "^1.2.2",
    "connect": "^3.7.0",
    "connect-redis": "^4.0.3",
    "eslint": "8.38.0",
    "fixpack": "^2.3.1",
    "istanbul": "^0.4.5",
    "koa": "2.16.2",
    "koa-generic-session": "^2.0.4",
    "lint-staged": "^9.5.0",
    "mocha": "3.x",
    "rimraf": "^3.0.0",
    "should": "^13.2.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/mattduffy/koa-redis",
  "keywords": [
    "cache",
    "caching",
    "client",
    "clients",
    "cluster",
    "clustering",
    "clusters",
    "express",
    "node-redis",
    "koa",
    "middleware",
    "multiple",
    "node",
    "nodes",
    "redis",
    "scale",
    "sentinal",
    "sentinals",
    "sentinel",
    "sentinels",
    "session",
    "storage",
    "store"
  ],
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "xo --fix",
        "git add"
      ],
      "*.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "lib/index.js",
  "type": "module",
  "repository": "koajs/koa-redis",
  "scripts": {
    "autod": "autod -w --prefix=^ --keep=koa -D co-mocha,mocha,istanbul,autod",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf lib",
    "build:lib": "babel src --out-dir lib",
    "lint": "xo && remark . -qfo && eslint lib",
    "test": "npm run build && npm run lint && istanbul cover node_modules/mocha/bin/_mocha -- --require co-mocha \"test/**/*.test.js\"",
    "test-only": "mocha --require co-mocha \"test/**/*.test.js\""
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "overrides": [
      {
        "files": "test/**/*.js",
        "envs": [
          "mocha"
        ]
      }
    ]
  }
}
